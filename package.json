{
  "name": "cypress-load-balancer",
  "version": "1.0.3",
  "author": "Zachary J. Hamm",
  "license": "MIT",
  "repository": "https://github.com/hammzj/cypress-load-balancer/",
  "bugs": {
    "url": "https://github.com/hammzj/cypress-load-balancer/issues"
  },
  "homepage": "https://github.com/hammzj/cypress-load-balancer#readme",
  "private": false,
  "keywords": [
    "cypress",
    "ci",
    "continuous-integration",
    "load-balancing",
    "parallel",
    "parallelization",
    "testing",
    "sharding"
  ],
  "main": "dist/cjs/index.js",
  "module": "dist/mjs/index.js",
  "types": "dist/mjs/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/mjs/index.d.ts",
      "import": "./dist/mjs/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "bin": {
    "cypress-load-balancer": "./dist/cjs/src/bin/cypressLoadBalancer.js"
  },
  "scripts": {
    "build": "tsc -p tsconfig.esm.json && tsc -p tsconfig.commonjs.json && ./scripts/fixup",
    "example:cypress:open": "cypress open --e2e",
    "example:cypress:run": "cypress run --e2e",
    "example:cypress:run:cucumber": "cypress run -C cypress.config.cucumber.ts --e2e",
    "example:cypress:run:1/2": "cypress run --e2e --env runner=1/2",
    "example:cypress:run:2/2": "cypress run --e2e --env runner=2/2",
    "merge-maps": "npx cypress-load-balancer merge -G \".cypress_load_balancer/spec-map-*-*.json\"",
    "dev:check": "tsc -p tsconfig.json",
    "dev:check:tests": "tsc -p tsconfig.test.json",
    "dev:format": "prettier --write . && prettier --write \".github/**/*.yml\"",
    "dev:lint": "eslint . && prettier --check . && prettier --check \".github/**/*.yml\"",
    "test": "NO_COLOR=1 FORCE_COLOR=0 mocha",
    "test:skip-long-tests": "NO_COLOR=1 FORCE_COLOR=0 SKIP_LONG_TESTS=1 mocha",
    "pretest": "yarn build",
    "prepare": "husky",
    "prepublish": "yarn build"
  },
  "files": [
    "dist/cjs",
    "dist/mjs",
    "LICENSE",
    "README.md"
  ],
  "peerDependencies": {
    "cypress": ">= 10.11.0"
  },
  "dependencies": {
    "@actions/core": "^1.11.1",
    "debug": "^4.4.3",
    "deepmerge": "^4.3.1",
    "find-cypress-specs": "^1.47.4",
    "glob": "10.4.5",
    "ts-node": "^10.9.2",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@badeball/cypress-cucumber-preprocessor": "^22.2.0",
    "@bahmutov/cypress-esbuild-preprocessor": "^2.2.7",
    "@eslint/js": "^9.20.0",
    "@types/chai-string": "^1.4.5",
    "@types/cypress": "^1.1.6",
    "@types/debug": "^4.1.12",
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.13.4",
    "@types/sinon": "^17.0.3",
    "@types/sinon-chai": "^4.0.0",
    "@types/yargs": "^17.0.33",
    "chai": "^5.2.0",
    "chai-string": "^1.5.0",
    "cypress": "^14.5.4",
    "cypress-on-fix": "^1.1.0",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-cypress": "^4.1.0",
    "eslint-plugin-mocha": "^10.5.0",
    "globals": "^15.15.0",
    "husky": "^9.1.7",
    "mocha": "^11.1.0",
    "pinst": "^3.0.0",
    "prettier": "^3.5.1",
    "sinon": "^19.0.2",
    "sinon-chai": "^4.0.0",
    "strip-ansi": "^7.1.2",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.24.1"
  }
}
